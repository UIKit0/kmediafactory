<customwidgets>
</customwidgets>
<connections>
    <connection>
        <sender>okButton</sender>
        <signal>clicked()</signal>
        <receiver>DVDInfoLayout</receiver>
        <slot>accept()</slot>
    </connection>
    <connection>
        <sender>dvdListView</sender>
        <signal>currentChanged(QListViewItem*)</signal>
        <receiver>DVDInfoLayout</receiver>
        <slot>itemChanged(QListViewItem*)</slot>
    </connection>
    <connection>
        <sender>url</sender>
        <signal>returnPressed()</signal>
        <receiver>DVDInfoLayout</receiver>
        <slot>open()</slot>
    </connection>
    <connection>
        <sender>url</sender>
        <signal>urlSelected(const QString&amp;)</signal>
        <receiver>DVDInfoLayout</receiver>
        <slot>open()</slot>
    </connection>
    <connection>
        <sender>url</sender>
        <signal>openFileDialog(KURLRequester*)</signal>
        <receiver>DVDInfoLayout</receiver>
        <slot>configureFileDialog(KURLRequester*)</slot>
    </connection>
</connections>
<tabstops>
    <tabstop>dvdListView</tabstop>
</tabstops>
<slots>
    <slot access="protected">open()</slot>
    <slot access="protected">itemChanged(QListViewItem*)</slot>
    <slot access="protected">configureFileDialog(KURLRequester*)</slot>
</slots>
<layoutdefaults spacing="6" margin="11"/>
<layoutfunctions spacing="KDialog::spacingHint" margin="KDialog::marginHint"/>
</UI>
