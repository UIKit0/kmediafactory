<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN"
          "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
    <interface name="org.kde.kmediafactory.KMediaFactory">
        <method name="openProject">
            <arg name="file" type="s" direction="in"/>
        </method>
        <method name="newProject" />
        <method name="quit" />
        <method name="setProjectTitle">
            <arg name="title" type="s" direction="in"/>
        </method>
        <method name="setProjectType">
            <arg name="type" type="s" direction="in"/>
        </method>
        <method name="setProjectDirectory">
            <arg name="dir" type="s" direction="in"/>
        </method>
        <method name="projectTitle">
            <arg type="s" direction="out"/>
        </method>
        <method name="projectType">
            <arg type="s" direction="out"/>
        </method>
        <method name="projectDirectory">
            <arg name="subdir" type="s" direction="in"/>
            <arg type="s" direction="out"/>
        </method>
        <method name="addMediaObject">
            <arg name="xml" type="s" direction="in"/>
        </method>
        <method name="selectTemplate">
            <arg name="xml" type="s" direction="in"/>
        </method>
        <method name="selectOutput">
            <arg name="xml" type="s" direction="in"/>
        </method>
        <method name="start" />

        <method name="getOpenFileName">
            <arg name="startDir" type="s" direction="in"/>
            <arg name="filter" type="s" direction="in"/>
            <arg name="caption" type="s" direction="in"/>
            <arg type="s" direction="out"/>
        </method>
        <method name="debug">
            <arg name="txt" type="s" direction="in"/>
        </method>
    </interface>
</node>
