<?xml version="1.0"?>
<mime-info xmlns='http://www.freedesktop.org/standards/shared-mime-info'>
  <mime-type type="application/x-kmediafactory">
    <comment>KMediaFactory Project File</comment>
    <comment xml:lang="fi">KMediaFactory projekti tiedosto</comment>
    <sub-class-of type="application/xml"/>
    <glob pattern="*.kmf"/>
  </mime-type>
  <mime-type type="application/x-kmediafactory-template">
    <comment>KMediaFactory Template File</comment>
    <comment xml:lang="fi">KMediaFactory mallipohja tiedosto</comment>
    <sub-class-of type="application/zip"/>
    <glob pattern="*.kmft"/>
  </mime-type>
</mime-info
